otel-logs-pipeline:
  buffer:
    bounded_blocking:
      batch_size: 200
      buffer_size: 50000
  processor:
  - date:
      destination: '@timestamp'
      from_time_received: true
  sink:
  - opensearch:
      hosts:
      - https://opensearch-paas.kr-west1.dev2.samsungsdscloud.com
      index: cvc-logs-%{yyyy.MM.dd}
      index_type: custom
      insecure: true
      password: '!CVC_PW123'
      username: cvc_data
  source:
    otel_logs_source:
      authentication:
        unauthenticated:
      port: 21892
      ssl: false
  workers: 3
