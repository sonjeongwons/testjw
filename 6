import requests

# 파일 이름 및 경로
file_name = "fail.zip"
file_path = "/path/to/fail.zip"  # 파일의 실제 경로를 입력

# API URL 및 헤더 설정
url = "http://172.28.128.13:8081/api/v1/scanner/multipart-scan"
headers = {
    "X-CVC-API-KEY": "CVCKEY"  # API 키
}

# 파일 업로드를 위한 멀티파트 데이터 준비
files = {
    "file": (file_name, open(file_path, "rb"))
}

# API 호출
response = requests.post(url, headers=headers, files=files)

# 응답 출력
print("Status Code:", response.status_code)
try:
    print("Response Body:", response.json())
except Exception as e:
    print("Response Body:", response.text)
